{
  "name": "redash-client",
  "version": "11.0.0-dev",
  "description": "The frontend part of Redash.",
  "main": "index.js",
  "scripts": {
    "start": "SET NODE_OPTIONS=--openssl-legacy-provider && npm-run-all --parallel watch:viz webpack-dev-server",
    "clean": "rm -rf ./client/dist/",
    "build:viz": "(cd viz-lib && yarn build:babel)",
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && yarn build:viz &&  webpack",
    "watch:app": "SET NODE_OPTIONS=--openssl-legacy-provider && webpack --watch --progress --colors -d",
    "watch:viz": "(cd viz-lib && yarn watch:babel)",
    "watch": "SET NODE_OPTIONS=--openssl-legacy-provider  && npm-run-all --parallel watch:*",
    "webpack-dev-server": "webpack-dev-server",
    "prettier": "prettier --write 'client/app/**/*.{js,jsx,ts,tsx}' 'client/cypress/**/*.{js,jsx,ts,tsx}'",
    "type-check": "tsc --noEmit --project client/tsconfig.json",
    "type-check:watch": "yarn type-check --watch",
    "preinstall": "cd viz-lib && yarn link --link-folder ../.yarn",
    "postinstall": "(cd viz-lib && yarn add && yarn build:babel) && yarn link --link-folder ./.yarn @redash/viz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getredash/redash.git"
  },
